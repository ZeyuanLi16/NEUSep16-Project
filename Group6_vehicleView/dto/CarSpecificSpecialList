package org.neu.project.dto;

import java.util.ArrayList;
import java.util.Date;
import java.util.HashMap;

//dto

//Author Anjali
//edited by Zeyuan Li
public class CarSpecificSpecialList {
	//edit by Zeyuan
	//begin
	//String splID;
	//public Double specialledPrice;
	//public String expiry;
	private String splID;
	private Double specialledPrice;
	private Date expiry;
	private Special special;

	//CarSpecificSpecialList(Special spl, double specialledPrice, Date expiry) {
	CarSpecificSpecialList(Special spl, double specialledPrice, Date expiry, Special special) {
		this.special=special;
	//end
		this.splID = spl.toString();
		this.specialledPrice = Double.valueOf(specialledPrice);
		this.expiry = expiry;
		ArrayList<Object> al = new ArrayList<Object>();
		al.add(this.specialledPrice);
		al.add(this.expiry);
		HashMap<String, ArrayList<Object>> hm = new HashMap<String, ArrayList<Object>>();
		hm.put(this.splID, al);
	}

	CarSpecificSpecialList() {

	}
	
	//edit by Zeyuan
	//begin

	public Date getExpiry() {
		return expiry;
	}

	public void setExpiry(Date expiry) {
		this.expiry = expiry;
	}

	public String getSplID() {
		return splID;
	}

	public void setSplID(String splID) {
		this.splID = splID;
	}

	public Double getSpecialledPrice() {
		return specialledPrice;
	}

	public void setSpecialledPrice(Double specialledPrice) {
		this.specialledPrice = specialledPrice;
	}

	public Special getSpecial() {
		return special;
	}

	public void setSpecial(Special special) {
		this.special = special;
	}
	//end
